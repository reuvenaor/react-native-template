{
  "name": "ExApp",
  "version": "0.0.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:expo": "expo run:android",
    "ios:expo": "expo run:ios",
    "lint": "eslint .",
    "format": "prettier --write .",
    "start": "react-native start",
    "test": "jest",
    "refresh:watchman": "watchman watch-del-all",
    "refresh:modules": "rm -rf node_modules/ && npm install",
    "refresh:start": "npm run start --reset-cache",
    "refresh": "npm run refresh:watchman && npm run refresh:modules && npm run refresh:start",
    "ios:install": "cd ios && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "expo:doctor": "npx expo-doctor",
    "expo:check": "npx expo install --check"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-vector-icons/ant-design": "^4.4.2",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.x",
    "@react-navigation/native-stack": "^6.x",
    "@reduxjs/toolkit": "^2.4.0",
    "@shopify/flash-list": "1.7.6",
    "@shopify/react-native-skia": "2.0.0-next.2",
    "pino-pretty": "^13.0.0",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-loading-spinner-overlay": "~3.0.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-sensors": "~7.3.6",
    "react-redux": "^9.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "tv4": "^1.3.0",
    "expo": "53.0.8"
  },
  "devDependencies": {
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "react-test-renderer": "19.0.0",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "metro-config": "^0.82.0",
    "@types/redux-logger": "^3.0.13",
    "@types/tv4": "^1.2.33",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "pino": "^9.5.0",
    "prettier": "2.8.8",
    "typescript": "^5.3.3",
    "typescript-json-schema": "^0.65.1"
  },
  "engines": {
    "node": ">=18"
  }
}